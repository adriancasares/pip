import axios from "axios";

export async function getStaticPaths() {
  const res = await axios.get(
    "https://lasapip.com/api/newsletter/getNewsletterPaths"
  );
  console.log(res.data);

  return res.data["newsletterPaths"].map((path: string) => {
    return {
      params: {
        slug: path,
      },
    };
  });
}
